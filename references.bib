@article{millanInstitutionalisationSquadrismoDisciplining2013,
  title = {The {{Institutionalisation}} of "{{Squadrismo}}": {{Disciplining Paramilitary Violence}} in the {{Italian Fascist Dictatorship}}},
  author = {family=MILLAN, given=MATTEO, given-i=MATTEO},
  date = {2013},
  journaltitle = {Contemporary European History},
  volume = {22},
  number = {4},
  eprint = {43299403},
