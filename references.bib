@article{hondaSocialWithdrawalPersons2013,
  title = {Social Withdrawal of Persons with Vascular Dementia Associated with Disturbance of Basic Daily Activities, Apathy, and Impaired Social Judgment},
  author = {Honda, Yukiko and Meguro, Kenichi and Meguro, Mitsue and Akanuma, Kyoko},
