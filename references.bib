  author = {O'Connor, Deborah and Phinney, Alison and Smith, Andre and Small, Jeff and Purves, Barbara and Perry, Joann and Drance, Elisabeth and Donnelly, Martha and Chaudhury, Habib and Beattie, Lynn},
