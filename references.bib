@article{wilderDidResilienceSocioeconomic2023,
  title = {Did {{Resilience}} and {{Socioeconomic Status Predict Older Adults}}' {{Finding}} a {{Silver Lining}} in {{COVID}}?},
  author = {Wilder, Jocelyn and Lauderdale, Diane S and Hawkley, Louise},
  date = {2023-07-25},
  journaltitle = {Innovation in Aging},
  shortjournal = {Innovation in Aging},
  volume = {7},
