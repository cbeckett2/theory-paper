  with = {Kilmer, Greta and Omura, John D. and Bouldin, Erin D. and Walker, Jenny and Spears, Katie and Gore, Janelle and Ali, Akilah R. and McGuire, Lisa C.},
  date = {2024-08-29},
  journaltitle = {Centers for Disease Control and Prevention},
  shortjournal = {MMWR Morb. Mortal. Wkly. Rep.},
  series = {{{MMWR}}. {{Morbidity}} and {{Mortality Weekly Report}}},
