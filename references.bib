@inproceedings{beckettDevelopingLayExpertise2025,
  title = {Developing Lay Expertise in Informal Dementia Caregiving: An Integrative Qualitative Approach},
  booktitle = {Associazione Italiana di Sociologia Meta Mandato},
  author = {Beckett, Cameron},
  date = {2025},
  location = {Forli},
  abstract = {Introduction: Lay expertise---the knowledge accumulated through caregiving experiences---is essential for navigating healthcare systems, understanding Alzheimer's disease and related dementias (ADRD), accessing support services, and managing emotional and physical challenges. This expertise enhances the quality of life for caregivers and care recipients, requiring caregivers to embody their roles and reassess their identities. This study explores how caregivers develop lay expertise, focusing on the impact of disruptive events and changing perspectives within their life course. Methods: Using a multi-sited design, this research integrates narrative analysis, ethnography, grounded theory, and situational analysis to examine the ecology of ADRD caregiving. Data were gathered through 20 in-depth interviews in English and Italian, and ethnographic fieldwork at Rete Magica in Forli, Italy, and LifeStream Services in Indiana, U.S. Results: Grounded theory analysis revealed two primary themes: *Building Lay Expertise*, encompassing caregivers' acquisition of knowledge and skills through lived experiences and emotional bonds, and *Constellation of Burdens*, detailing caregivers' multifaceted challenges and stressors. Additionally, concepts of biographical disruption and moments of discovery illustrate how crisis events reshape caregivers' identities and life trajectories. Situational analysis mapped relationships among caregivers, social actors, and cultural norms, highlighting the development of embodied knowledge and evolving self-identity as crucial for managing responsibilities and mitigating burden. Analysis: Interpreted through Andrew Abbott's processual sociology, findings depict caregiving as a dynamic, evolving process shaped by continuous adaptation and socio-cultural interactions, underscoring the importance of embodied knowledge and self-efficacy in reducing caregiver burden and promoting well-being. Discussion: This study advocates for enhanced support systems and policies that facilitate the development of lay expertise. With OECD populations aging rapidly, demand on informal caregivers will increase, necessitating support to mitigate social withdrawal and isolation. Findings highlight perceived social support as pivotal in shaping caregiving experiences and reducing burden across different settings, contributing to discourse on dementia caregiving by providing empirically grounded insights into caregivers' strategies and challenges.},
  eventtitle = {Uno sguardo di lungo periodo: la salute in una prospettiva di corso di vita},
  langid = {italian}
}

