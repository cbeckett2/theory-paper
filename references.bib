  title = {Personalizing Solidarity? {{The}} Role of Self-Tracking in Health Insurance Pricing},
  shorttitle = {Personalizing Solidarity?},
  author = {McFall, Liz},
  date = {2019-01-02},
  journaltitle = {Economy and Society},
  shortjournal = {Economy and Society},
  volume = {48},
  number = {1},
