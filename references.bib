@article{jacobsohnFactorsInfluencingEmergency2019,
  title = {Factors {{Influencing Emergency Care}} by {{Persons}} with {{Dementia}}: {{Stakeholder Perceptions}} and {{Unmet Needs}}},
  shorttitle = {Factors {{Influencing Emergency Care}} by {{Persons}} with {{Dementia}}},
  author = {Jacobsohn, Gwen Costa and Hollander, Matthew and Beck, Aaron P and Gilmore-Bykovskyi, Andrea and Werner, Nicole and Shah, Manish N.},
  date = {2019-04},
  journaltitle = {Journal of the American Geriatrics Society},
  shortjournal = {J Am Geriatr Soc},
  volume = {67},
