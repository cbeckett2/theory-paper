@article{wilterdinkDrivingDeadendStreet2018,
  title = {Driving in a Dead-End Street: Critical Remarks on {{Andrew Abbott}}'s {{Processual Sociology}}},
  shorttitle = {Driving in a Dead-End Street},
  author = {Wilterdink, Nico},
  date = {2018-08-01},
  journaltitle = {Theory and Society},
  shortjournal = {Theor Soc},
  volume = {47},
  number = {4},
