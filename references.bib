  keywords = {Adaptation Psychological,Aged,Alzheimer Disease,caregiver burden,Caregivers,caregiving self-efficacy,dementia,depression,Depression,Family,Female,Humans,Italy,Longitudinal Studies,Male,Mental Health,Middle Aged,Self Efficacy},
