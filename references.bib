@article{ruggianoFeasibilityUsingPrivate2021,
  title = {Feasibility of {{Using Private Facebook Groups}} for {{Community-Based Dementia Caregiver Support}}},
  author = {Ruggiano, Nicole and Washington, Karla T. and Luo, Yan and Noh, Hyunjin and Brown, Ellen L.},
  date = {2021-03-01},
  journaltitle = {Best Practices in Mental Health},
  shortjournal = {Best Practices in Mental Health},
  volume = {17},
  number = {1},
  pages = {44--55},
  abstract = {Objective: Dementia caregivers often experience negative physical and mental health symptoms due to their caregiving duties; they may also experience barriers to attending in-person education and support that may reduce burden and depression. This pilot intervention study examined the feasibility of using a private Facebook group to deliver virtual support to dementia caregivers. Method: A twelve-week support group intervention was implemented using a private Facebook group as a platform. Data collection included preinterviews and postinterviews with participants. A thematic analysis of data was conducted and interactions within the group were assessed. Results: Eight caregivers in Alabama enrolled in the program, including African American and rural-dwelling caregivers. Findings indicate that caregivers had a positive experience in the group, found group content relevant and helpful, and were comfortable interacting with others in the group because it was private. Conclusions: This pilot study demonstrated that private Facebook groups are a feasible way of providing community-based education and support for dementia caregivers. Given their accessibility, wide use, and cost effectiveness, service providers may consider how they may transfer some services to a virtual platform using private Facebook groups to reach caregivers who are at high risk of isolation or have other barriers to accessing in-person services.},
  keywords = {CAREGIVERS,DEMENTIA,dissertation-full,SELF-HELP GROUPS,SOCIAL MEDIA,TECHNOLOGY}
}

