@article{jacobsDementiaSouthAfrica2024,
  title = {Dementia in {{South Africa}}: {{A Situational Analysis}}},
  shorttitle = {Dementia in {{South Africa}}},
  author = {Jacobs, Roxanne and Schneider, Marguerite and Farina, Nicolas and Du Toit, Petra and Docrat, Sumaiyah and Comas-Herrera, Adelina and Knapp, Martin},
