@article{kriegerEmbodyingInequalityReview1999a,
  title = {Embodying {{Inequality}}: {{A Review}} of {{Concepts}}, {{Measures}}, and {{Methods}} for           {{Studying Health Consequences}} of {{Discrimination}}},
