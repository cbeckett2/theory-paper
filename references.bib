  shorttitle = {{{GIVE A PIECE OF YOUR MIND}}},
  author = {Lagervall, Jenny A and Lag, Madeline R and Brickman, Sophie and Ingram, Rebecca E and Feliciano, Leilani},
  date = {2019-11-08},
  journaltitle = {Innovation in Aging},
