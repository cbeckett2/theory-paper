  title = {Comparative Study on Informal Caregiver Support Policies in the Long-Term Care System for Older Adults among {{South Korea}}, the {{United Kingdom}}, and {{Sweden}}},
  author = {Kim, Hyeji and Ko, Sung Hyun and Park, Sanghee},
