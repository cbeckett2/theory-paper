@article{mittelmanImprovingCaregiverWellbeing2006,
  title = {Improving Caregiver Well-Being Delays Nursing Home Placement of Patients with {{Alzheimer}} Disease},
  author = {Mittelman, Mary S. and Haley, William E. and Clay, Olivio J. and Roth, David L.},
  date = {2006-11-14},
