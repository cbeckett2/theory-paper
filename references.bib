@book{hansenNotSoNuclearFamiliesClass2005,
  title = {Not-{{So-Nuclear Families}}: {{Class}}, {{Gender}}, and {{Networks}} of {{Care}}},
  shorttitle = {Not-{{So-Nuclear Families}}},
