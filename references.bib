  author = {family=Wijngaarden, given=Els, prefix=van, useprefix=true and family=Wedden, given=Hugo, prefix=van der, useprefix=true and Henning, Zerline and Komen, Rikke and The, Anne-Mei},
  date = {2018-06-13},
  journaltitle = {PLoS ONE},
  shortjournal = {PLoS One},
