@article{ginnUnderstandingCaregiverBurden2024,
  title = {Understanding {{Caregiver Burden}} in {{Decision-Making}} in {{Alzheimer}}'s {{Disease}} and {{Related Dementias}}},
  author = {Ginn, Brandi and Arteaga, Ignacia and family=Jes\'us, given=Alma Hern\'andez, prefix=de, useprefix=true and Abramson, Corey and Dohan, Daniel},
  date = {2024-12-31},
