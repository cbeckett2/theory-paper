  author = {Chiatti, Carlos and Furneri, Gianluca and Rimland, Joseph M. and Demma, Federica and Bonfranceschi, Franco and Cassetta, Laura and Masera, Filippo and Cherubini, Antonio and Corsonello, Andrea and Lattanzio, Fabrizia and {on behalf of the UP-TECH research group}},
  date = {2015-09},
  journaltitle = {International Psychogeriatrics},
  shortjournal = {Int. Psychogeriatr.},
  volume = {27},
  number = {9},
  pages = {1563--1572},
  issn = {1041-6102, 1741-203X},
  doi = {10.1017/S104161021500040X},
  url = {https://www.cambridge.org/core/product/identifier/S104161021500040X/type/journal_article},
  urldate = {2023-02-21},
  abstract = {Background: There is consensus that dementia is the most burdensome disease for modern societies. Few cost-of-illness studies examined the complexity of Alzheimer's disease (AD) burden, considering at the same time health and social care, cash allowances, informal care, and out-of-pocket expenditure by families. Methods: This is a comprehensive cost-of-illness study based on the baseline data from a randomized controlled trial (UP-TECH) enrolling 438 patients with moderate AD and their primary caregiver living in the community. Results: The societal burden of AD, composed of public, patient, and informal care costs, was about \texteuro 20,000/yr. Out of this, the cost borne by the public sector was \texteuro 4,534/yr. The main driver of public cost was the national cash-for-care allowance (\texteuro 2,324/yr), followed by drug prescriptions (\texteuro 1,402/yr). Out-of-pocket expenditure predominantly concerned the cost of private care workers. The value of informal care peaked at \texteuro 13,590/yr. Socioeconomic factors do not influence AD public cost, but do affect the level of out-of-pocket expenditure. Conclusion: The burden of AD reflects the structure of Italian welfare. The families predominantly manage AD patients. The public expenditure is mostly for drugs and cash-for-care benefits. From a State perspective in the short term, the advantage of these care arrangements is clear, compared to the cost of residential care. However, if caregivers are not adequately supported, savings may be soon offset by higher risk of caregiver morbidity and mortality produced by high burden and stress. The study has been registered on the website www.clinicaltrials.org (Trial Registration number: NCT01700556).},
  langid = {english},
  file = {C:\Users\Cameron\Zotero\storage\G6WDB39J\Chiatti et al. - 2015 - The economic impact of moderate stage Alzheimer's .pdf}
}

