@article{ryffHappinessEverythingIt1989,
  title = {Happiness Is Everything, or Is It? {{Explorations}} on the Meaning of Psychological Well-Being},
  shorttitle = {Happiness Is Everything, or Is It?},
  author = {Ryff, Carol D.},
  date = {1989},
  journaltitle = {Journal of Personality and Social Psychology},
  volume = {57},
  number = {6},
  pages = {1069--1081},
  publisher = {American Psychological Association},
